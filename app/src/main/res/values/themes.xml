<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.OnTimeUvers" parent="Theme.Material3.DayNight.NoActionBar">

        <item name="android:fontFamily">@font/creatodisplayblack</item>
        <item name="android:textColor">@color/blacks</item>
        <item name="colorPrimary">@color/blue</item>

    </style>

    <style name="Theme.OnTimeUvers" parent="Base.Theme.OnTimeUvers" />

    <style name="Theme.MyOverlay" parent="">
        <item name="colorPrimary">#E75D5D</item>
    </style>

    <style name="Theme.tab" parent="">
        <item name="colorPrimary">#DDDDDD</item>
        <item name="android:radius">0dp</item>
    </style>

    <style name="Theme.table" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:fontFamily">@font/creatodisplayblack</item>
        <item name="android:textColor">@color/blacks</item>
        <item name="buttonStyle">@style/Table</item>

        <item name="materialButtonStyle">@style/Table</item>
    </style>
    <style name="Theme.tables" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:fontFamily">@font/creatodisplayblack</item>
        <item name="android:textColor">@color/blacks</item>
        <item name="buttonStyle">@style/Tables</item>

        <item name="materialButtonStyle">@style/Tables</item>
    </style>


</resources>